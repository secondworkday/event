  "Tags": [
    {
      "Type": "Performance",
      "Settings": {
        "SampleIntervalInSeconds": "5",
        "SamplesToConsider": "6",
        "Triggers": [
          {
            "Name": "High CPU",
            "Description": "High CPU usage",
            "PerfCounter": "Processor Information\\% Processor Time\\_Total",
            "Operator": ">",
            "Metric": "70"
          },
          {
            "Name": "Busy Disk",
            "Description": "High disk usage",
            "PerfCounter": "PhysicalDisk\\% Disk Time\\_Total",
